<div class = account&profile>
  <%= form_with model: @user, local: true do |f| %>
  <table>
    <tr>
        <th><%= f.label :name ,"名前" %></th>
        <td><%= f.text_field :name %></td>
    </tr>
    <tr>
        <th><%= f.label :image ,"プロフィール画像" %></th>
        <td><%= f.file_field :image_name %></td>
    </tr>
    <tr>
        <th><%= f.label :email ,"メールアドレス" %></th>
        <td><%= f.text_field :email %></td>
    </tr>
    <tr>
        <th><%= f.label :password ,"パスワード" %></th>
        <td><%= f.text_area :password %></td>
    </tr>
    <tr>
        <th><%= f.label :password_confirmation ,"パスワード確認" %></th>
        <td><%= f.text_area :password_confirmation %></td>
    </tr>
  </table>
<div>
  <ul>
    <li><%= f.submit "編集する"%></li>
    <li><%= link_to "宿泊先一覧に戻る", "#" %></li>
  </ul>
</div>
    <% end %>
</div>